@model WebApplication1.Models.Organization

@{
    ViewData["Title"] = "AddOrg";
}


<form method="post">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Organization Name</label>
        <input type="text" asp-for="OrganizationName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Email </label>
        <input type="email" asp-for="CompaneyEmail" class="form-control" id="exampleInputPassword1">
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Founded Date </label>
        <input type="text" asp-for="FoundedDate" class="form-control" id="exampleInputPassword1">
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Phone </label>
        <input type="text" asp-for="Phone" class="form-control" id="exampleInputPassword1">
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Location </label>
        <input type="text" asp-for="Location" class="form-control" id="exampleInputPassword1">
    </div>

    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>